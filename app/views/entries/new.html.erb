<h1>Create New Entry</h1>

	<%= form_for [@project, @entry] do |f| %>

		Hour: <%= f.number_field :hour %><br />
		Minute : <%= f.number_field :minute %><br />
		Create at : <%= f.date_field :created_at %><br />
		Comment : <%= f.text_area :comment %><br />

	<%= f.submit "Create Entry" %>

<% end %>